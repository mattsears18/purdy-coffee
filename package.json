{
  "name": "purdy-coffee",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://mattsears18.github.io/purdy-coffee",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test-with-coverage": "react-scripts test --coverage && cat ./coverage/lcov.info | codacy-coverage --token cadc4540bce847d0a149e93582bd56d4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "gh-pages": "^2.0.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "react-styleguidist": "^9.0.9",
    "source-map-explorer": "^1.8.0"
  },
  "devDependencies": {
    "codacy-coverage": "^3.4.0",
    "coveralls": "^3.0.3",
    "husky": "^2.3.0"
  }
}
